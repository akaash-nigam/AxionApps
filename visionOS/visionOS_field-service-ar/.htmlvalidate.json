{
  "extends": ["html-validate:recommended"],
  "rules": {
    "void-style": "off",
    "close-attr": "error",
    "close-order": "error",
    "attr-quotes": "error",
    "doctype-html": "error",
    "no-dup-attr": "error",
    "no-dup-id": "error",
    "no-inline-style": "off",
    "require-sri": "off"
  }
}
